{
  "name": "base-vue-web",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "format": "prettier --write ./**/*.{vue,ts,tsx,js,jsx,css,less,scss,json,md}",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vueuse/core": "^9.0.2",
    "@vueuse/router": "^9.0.2",
    "dat.gui": "^0.7.9",
    "gsap": "^3.10.4",
    "three": "^0.142.0",
    "vue": "^3.2.37",
    "vue-router": "^4.1.2"
  },
  "devDependencies": {
    "@types/dat.gui": "^0.7.7",
    "@types/node": "^18.0.6",
    "@types/three": "^0.142.0",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.2.0",
    "espree": "^9.3.2",
    "glsl-noise": "^0.0.0",
    "husky": "^8.0.1",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "rollup": "2.77.0",
    "stylelint": "^14.9.1",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-less": "^1.0.6",
    "tailwindcss": "^3.1.6",
    "typescript": "^4.7.4",
    "vite": "^3.0.2",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vite3-plugin-glslify": "^0.0.7",
    "vue-tsc": "^0.38.9"
  },
  "lint-staged": {
    "*.{js,vue,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{html,css,less,scss,md}": [
      "prettier --write"
    ]
  }
}
